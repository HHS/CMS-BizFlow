	<queryString>
		<![CDATA[SELECT 
	INC."CANDI_NAME"  AS Applicant_Name,
	INC."ADMIN_CODE" AS Admin_Code,
	INC."ORG_NAME" AS Organization,
	INC."POS_TITLE" AS Title,
	INC."PAY_PLAN" AS Pay_Plan,
	INC."SERIES" AS Series,
	CASE 
    WHEN INC."PAY_PLAN" IN ('ES') THEN  ' '
    WHEN INC."PAY_PLAN" IN ('GP', 'GR', 'GS', 'WG') THEN  INC."GRADE" 
    ELSE 'N/A' 
  END AS Grade,
	CONCAT(CONCAT(LE."TOTAL_CREDITABLE_YEARS", ' year(s) and '), CONCAT(LE."TOTAL_CREDITABLE_MONTHS", ' month(s)'))
	 AS Service_Credited_Amt, 
	
	INC."SO_TITLE" As SO_Title,
	INC."HRS1_NAME" As  HRS1_NAME,
	INC."SO_NAME" As Selecting_Official,
	LE."COC_NAME" As COC_NAME,
	LE."APPROVAL_SO_VALUE" As APPROVAL_SO_VALUE,
	LE."APPROVAL_SO_ACTING" As APPROVAL_SO_ACTING,
	LE."APPROVAL_COC_ACTING" As APPROVAL_COC_ACTING,
	     
	LE."HRS_RVW_CERT" As HRS_RVW_CERT,
	LE."APPROVAL_TABG_RESP_DATE" As APPROVAL_TABG_RESP_DATE,
	LE."APPROVAL_TABG" As APPROVAL_TABG,
	LE."APPROVAL_TABG_ACTING" As APPROVAL_TABG_ACTING,
	LE."APPROVAL_TABG_VALUE" As APPROVAL_TABG_VALUE,
	LE."APPROVAL_COC_RESP_DATE" As APPROVAL_COC_RESP_DATE,
	LE."APPROVAL_SO_RESP_DATE" As APPROVAL_SO_RESP_DATE,
	LE."HRS_RVW_DATE" As HRS_RVW_DATE,
	LE."HRS_NOT_SPT_RSN" As HRS_NOT_SPT_RSN,
	LE."APPROVAL_COC_RESP_DATE" As APPROVAL_COC_RESP_DATE,
	LE."COC_TITLE" As COC_TITLE,
	    
	LE."PROPS_ANN_LA_RATE" As PROPS_ANN_LA_RATE,
    LE."JUSTIFICATION_SKILL_EXP" AS Justifications_Skills,
    LE."JUSTIFICATION_AGENCY_GOAL" AS Justification_Goals,
    LE."APPROVER_NOTES" AS APPROVER_NOTES
  
FROM HHS_CMS_HR.VW_INCENTIVES_COM INC
INNER JOIN  HHS_CMS_HR.VW_INCENTIVES_LE LE ON  INC.PROC_ID = LE.PROC_ID
WHERE INC.PROC_ID = $P{caseID}]]>
	</queryString>
